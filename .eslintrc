{
  "extends": ["react-app", "react-app/jest", "prettier", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
      "prettier/prettier": ["warn", {"singleQuote:": true, "printWidth": 120, "endOfLine": "auto"}],
      "important/first": 0
  },
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "parser": "@babel/eslint-parser"
}